class TestJoinThread extends Thread {
	//构造方法
	public TestJoinThread(String name) {
		super(name);
	}
	
	public void run() {
		for (int i = 0; i <= 5; i++) {
			System.out.println("I am at " + getName());
		}
	}
	
}