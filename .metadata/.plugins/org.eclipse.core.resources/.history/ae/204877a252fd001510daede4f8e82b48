 */
package com.frankstar.CollectionStudy;

/**
 * @author frankstar
 * 实现自己的Entry
 */
public class Entry<K, V> {
	final K key;
	V value;
	Entry<K, V> next; //下一个节点
	
	//构造函数
	public Entry(K k, V v, Entry<K, V> n)
	{
		key = k;
		value = v;
		next = n;
	}
	public final K getKey() 
	{
		return key;
	}
	public final V getValue()
	{
		return value;
	}
	public final V setValue(V nvalue)
	{
		V ovalue = value;
		value = nvalue;
		return ovalue;
	}
}
