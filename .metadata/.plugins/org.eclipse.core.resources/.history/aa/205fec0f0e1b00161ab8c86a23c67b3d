<script language="javascript" type="text/javascript">
	function MyCheck() {
		if (form.account.value == "") {
			alert("请输入用户名：");
			form.account.focus();
			return false;
		}
		if (form.password.value == "") {
			alert("请输入密码：");
			form.password.focus();
			return false;
		}
		form.submit();

  }
</script>
