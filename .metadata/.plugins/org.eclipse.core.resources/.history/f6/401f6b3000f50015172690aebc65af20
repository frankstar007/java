class Value {
	static int c = 0;
	Value(){
		c = 15;
	}
	Value(int i) {
		c = i;
	}
	static void inc () {
		c++;
	}
}
class Count {
	public static void prt(String s) {
		System.out.println(s);
	}
	Value v = new Value(10);
	static Value v1, v2;
	static {
		prt("v1.c="+ v1.c + " v2.c= " + v2.c)
	}
	
}
